{
  "name": "@humbledao/nuxt-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "yarn contracts:build",
    "dev": "cross-env DEPLOYED_NET_ENV=local nuxt",
    "dev:testnet": "cross-env DEPLOYED_NET_ENV=testnet nuxt",
    "dev:mainnet": "cross-env DEPLOYED_NET_ENV=mainnet nuxt",
    "build": "cross-env DEPLOYED_NET_ENV=mainnet nuxt build",
    "start": "nuxt start",
    "generate": "cross-env DEPLOYED_NET_ENV=testnet nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix",
    "test": "jest",
    "contracts:test-development": "truffle test --network development",
    "contracts:build": "truffle compile --all",
    "contracts:deploy-development": "truffle migrate --network development",
    "contracts:deploy-development-wsl": "truffle migrate --network development-wsl"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.4.1",
    "@ensdomains/ensjs": "^2.1.0",
    "@metamask/detect-provider": "^1.2.0",
    "@nuxtjs/axios": "^5.13.6",
    "@openzeppelin/contracts": "^4.6.0",
    "@openzeppelin/contracts-upgradeable": "^4.6.0",
    "@sushiswap/sdk": "^5.0.0-canary.87",
    "@toruslabs/torus-embed": "^1.13.6",
    "@walletconnect/client": "^1.7.8",
    "@walletconnect/qrcode-modal": "^1.7.8",
    "@walletconnect/web3-provider": "^1.7.8",
    "abdk-libraries-solidity": "3.0.0",
    "authereum": "^0.1.14",
    "bitski": "^0.14.1",
    "buefy": "^0.9.21",
    "core-js": "^3.22.5",
    "date-fns": "^2.27.0",
    "date-fns-tz": "^1.1.7",
    "graphql": "^16.5.0",
    "lodash": "^4.17.21",
    "nuxt": "^2.15.8",
    "nuxt-buefy": "^0.4.13",
    "truffle-flatten": "^1.0.8",
    "vue": "^2.6.14",
    "vue-date-pick": "^1.4.1",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "web3": "^1.7.3",
    "web3modal": "^1.9.7",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@openzeppelin/truffle-upgrades": "^1.15.0",
    "@truffle/config": "^1.3.28",
    "@truffle/hdwallet-provider": "^1.5.1",
    "@truffle/provider": "^0.2.54",
    "@types/chai": "^4.3.1",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.0.0",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.4.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-class-members": "^1.14.1",
    "eslint-plugin-tailwindcss": "^1.17.2",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.4.4",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "sass": "^1.52.1",
    "sass-loader": "10",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.20.1",
    "truffle": "^5.5.15",
    "truffle-contract-size": "^2.0.1",
    "truffle-plugin-verify": "^0.5.25",
    "ts-jest": "^27.1.2",
    "vue-jest": "^3.0.4"
  }
}
